{
    "name": "crisvegadev/facturama-php-sdk",
    "description": "Facturama PHP SDK",
    "keywords": ["API", "PHP", "JSON", "Facturama"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "crisvegadev",
            "email": "contact@crisvega.dev"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.5",
        "vlucas/phpdotenv": "^5.4",
        "psr/container": "^2.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": ">=5.6|>=7.0|>=8.0",
        "phpmd/phpmd": "^2.4.3"
    },
    "autoload": {
        "psr-4": {
            "Crisvegadev\\Facturama\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crisvegadev\\Facturama\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
